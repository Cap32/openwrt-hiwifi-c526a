+
+		led_sys: blue {
+			label = "c526a:blue:sys";
+			gpios = <&tlmm 36 GPIO_ACTIVE_LOW>;
+		};
+
+		led_boot: red {
+			label = "c526a:red:sys";
+			gpios = <&tlmm 40 GPIO_ACTIVE_LOW>;
+			panic-indicator;
+		};
+	};
+
+	keys {
+		compatible = "gpio-keys";
+
+		reset {
+			label = "reset";
+			gpios = <&tlmm 18 GPIO_ACTIVE_LOW>;
+			linux,code = <KEY_RESTART>;
+		};
+	};
+};
